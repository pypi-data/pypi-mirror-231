[Unit]
Description=CENSOR basis to prosecute (OTP-CR-117/19)
Requires=network.target
After=network.target

[Service]
DynamicUser=True
Type=forking
User=bart
Group=bart
PIDFile=censor.pid
WorkingDirectory=/home/bart/.censor
ExecStart=/home/bart/.local/pipx/venvs/censor/bin/censor -d
